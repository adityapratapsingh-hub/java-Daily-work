import java.util.*;
public class alicemagicalshoes {
        public static void main(String[] args) {
            Scanner sc = new Scanner(System.in);
            
            int N = sc.nextInt();
            int[] stairs = new int[N];
            for (int i = 0; i < N; i++) {
                stairs[i] = sc.nextInt();
            }
            int count = 0;
            for (int stairCount : stairs) {
                if (stairCount % 3 == 0) {
                    count++;
                }
            }
            System.out.println(count);
        }
    
}
